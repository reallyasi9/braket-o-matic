<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res'
		type='net.exclaimindustries.paste.braket.client.resources.Resources' />
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>
	<g:FlowPanel>
		<g:ScrollPanel>
			<g:FlowPanel ui:field="rules" />
		</g:ScrollPanel>
		<g:FlowPanel styleName="{res.style.buttonContainer}">
			<g:Button ui:field="closeButton" styleName="{res.style.blueButton}"
				addStyleNames="{res.style.rightButton}">close</g:Button>
		</g:FlowPanel>
	</g:FlowPanel>

</ui:UiBinder> 