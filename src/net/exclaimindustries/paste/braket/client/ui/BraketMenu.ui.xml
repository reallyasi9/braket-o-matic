<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style
		type="net.exclaimindustries.paste.braket.client.ui.BraketMenu.Style">
			.panel {
				background: #ffffff; /* Old browsers */
				background: -moz-linear-gradient(top, #ffffff 0%, #e5e5e5 100%);
				/* FF3.6+ */
				background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #ffffff),
					color-stop(100%, #e5e5e5)); /* Chrome,Safari4+ */
				background: -webkit-linear-gradient(top, #ffffff 0%, #e5e5e5 100%);
				/* Chrome10+,Safari5.1+ */
				background: -o-linear-gradient(top, #ffffff 0%, #e5e5e5 100%);
				/* Opera 11.10+ */
				background: -ms-linear-gradient(top, #ffffff 0%, #e5e5e5 100%);
				/* IE10+ */
				background: linear-gradient(to bottom, #ffffff 0%, #e5e5e5 100%);
				height: 32pt;
				overflow: overlay;
			}
			
			.menuButton {
				display: inline-block;
				height: 32pt;
				line-height: 32pt;
				vertical-align: center;
				padding-left: 8pt;
				padding-right: 8pt;
				cursor: pointer;
				border-right: 1px solid #888888;
				font-size: 14pt;
			}
			
			.menuButton a:link,.menuButton a:hover,.menuButton a:active,.menuButton a:visited
				{
				text-decoration: none;
				color: #666666 !important;
			}
			
			.adminButton {
				float: right;
				border-left: 1px solid #888888;
				border-right: 0px;
			}
		</ui:style>
	<g:FlowPanel styleName="{style.panel}" ui:field="panel" />
</ui:UiBinder> 