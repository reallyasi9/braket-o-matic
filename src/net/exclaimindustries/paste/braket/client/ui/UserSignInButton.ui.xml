<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="res"
		type="net.exclaimindustries.paste.braket.client.resources.Resources" />
	<ui:style>
		.panel {
			display: inline-block;
			float: right;
			right: 12pt;
			top: 25pt;
			position: relative;
		}
		
		.button {
			border: 2pt solid;
			display: inline-block;
			font-size: 12pt;
			font-weight: bold;
			line-height: 18pt;
			overflow: hidden;
			text-decoration: none;
			text-shadow: 0 1pt rgba(0, 0, 0, 0.5), 0 0 2pt rgba(0, 0, 0, 0.2);
			-moz-border-radius: 2pt;
			-ms-border-radius: 2pt;
			-o-border-radius: 2pt;
			-webkit-border-radius: 2pt;
			border-radius: 2pt;
			-moz-box-shadow: 0 1pt 0 rgba(0, 0, 0, 0.2);
			-ms-box-shadow: 0 1pt 0 rgba(0, 0, 0, 0.2);
			-o-box-shadow: 0 1pt 0 rgba(0, 0, 0, 0.2);
			-webkit-box-shadow: 0 1pt 0 rgba(0, 0, 0, 0.2);
			box-shadow: 0 1pt 0 rgba(0, 0, 0, 0.2);
			cursor: pointer;
			padding-right: 4pt;
			padding-left: 4pt;
			background-color: #606c88;
			border-color: #606c88;
			color: #e3eaed !important;
			text-decoration: none !important;
		}
		
		.button:active,.button:visited,.button:hover {
			color: #e3eaed !important;
			text-decoration: none !important;
		}
	</ui:style>
	<g:FlowPanel styleName="{style.panel}">
		<g:Anchor styleName="{style.button}" ui:field="signInAnchor">Sign In</g:Anchor>
	</g:FlowPanel>
</ui:UiBinder> 