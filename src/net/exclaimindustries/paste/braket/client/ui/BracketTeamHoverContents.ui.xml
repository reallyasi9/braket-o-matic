<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<div class="bracket-popup-team">
			<div class="bracket-popup-team-row">
				<img ui:field="teamIcon" class="bracket-popup-team-icon"></img>
			</div>
			<div class="bracket-popup-team-row">
				<div class="bracket-popup-team-name" ui:field="teamName"></div>
				<div class="bracket-popup-hint-text" ui:field="hintText">Click on any game to pick a team for it!</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 