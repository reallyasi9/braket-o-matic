<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field='res'
		type='net.exclaimindustries.paste.braket.client.resources.Resources' />

	<ui:style>
		
	</ui:style>
	<g:FlowPanel>
		<g:FlowPanel ui:field="rulesHere" />
		<g:FlowPanel styleName="{res.style.buttonContainer}">
			<g:Button ui:field="cancelButton" styleName="{res.style.blueButton}"
