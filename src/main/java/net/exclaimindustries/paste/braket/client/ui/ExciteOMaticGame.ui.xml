<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style
		type="net.exclaimindustries.paste.braket.client.ui.ExciteOMaticGame.MyStyle">
			.exciteGame {
				-webkit-border-radius: 6px;
				-moz-border-radius: 6px;
				border-radius: 6px;
				-webkit-box-shadow: 2px 2px 6px 0px rgba(0, .2, 0, .3);
				box-shadow: 2px 2px 6px 0px rgba(0, .2, 0, .3);
				background: rgb(238, 238, 238); /* Old browsers */
				background: -moz-linear-gradient(top, rgb(238, 238, 238) 0%,
					rgb(238, 238, 238) 1%, rgb(221, 221, 221) 100%); /* FF3.6+ */
				background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgb(238,
					238, 238)), color-stop(1%, rgb(238, 238, 238)),
					color-stop(100%, rgb(221, 221, 221))); /* Chrome,Safari4+ */
				background: -webkit-linear-gradient(top, rgb(238, 238, 238) 0%,
					rgb(238, 238, 238) 1%, rgb(221, 221, 221) 100%);
				/* Chrome10+,Safari5.1+ */
				background: -o-linear-gradient(top, rgb(238, 238, 238) 0%,
					rgb(238, 238, 238) 1%, rgb(221, 221, 221) 100%); /* Opera 11.10+ */
				background: -ms-linear-gradient(top, rgb(238, 238, 238) 0%,
					rgb(238, 238, 238) 1%, rgb(221, 221, 221) 100%); /* IE10+ */
				background: linear-gradient(to bottom, rgb(238, 238, 238) 0%,
					rgb(238, 238, 238) 1%, rgb(221, 221, 221) 100%); /* W3C */
				padding: 6px;
			}
			
			.headerCell {
				font-size: 14pt;
				height: 16pt;
				font-weight: bold;
				text-shadow: -1px -1px 1px #dddddd;
				color: 999999;
				width: 100%;
				text-align: center;
				line-height: 16pt;
			}
			
			.teamName {
				height: 14pt;
				font-size: 12pt;
				font-weight: bold;
				text-shadow: -1px -1px 1px #dddddd;
				color: 999999;
				text-align: center;
				line-height: 14pt;
			}
			
			.vs {
				font-size: 10pt;
				text-align: center;
				line-height: 14pt;
			}
			
			.teamImage {
				height: 110px;
				vertical-align: middle;
				height: 110px;
				width: 110px;
			}
			
			.arrowImage {
				height: 110px;
				width: 50px;
				width: 20px;
				margin: auto;
				height: 20px;
			}
			
			.teamChange {
				height: 12pt;
				font-size: 11pt;
				line-height: 11pt;
				text-align: center;
			}
			
			.positive {
				color: #00cc00;
			}
			
			.negative {
				color: #cc0000;
			}
			
			.leftArrow {
				background:
					url(data:image/png;base64,);
			}
			
			.rightArrow {
				background:
					url(data:image/png;base64,);
			}
		</ui:style>

	<g:LayoutPanel styleName="{style.exciteGame}" width="360px"
		height="180px">
		<g:layer left="6px" width="33%" top="18pt" height="14pt">
			<g:Label ui:field="topTeamName" styleName="{style.teamName}" />
		</g:layer>
		<g:layer left="33%" width="33%" top="25pt" height="12pt">
			<g:Label styleName="{style.vs}">vs</g:Label>
		</g:layer>
		<g:layer right="6px" width="33%" top="18pt" height="14pt">
			<g:Label ui:field="bottomTeamName" styleName="{style.teamName}" />
		</g:layer>
		<g:layer left="6px" width="33%" top="32pt" height="110px">
			<g:Image styleName="{style.teamImage}" ui:field="topTeamImage" />
		</g:layer>
		<g:layer left="33%" width="33%" top="32pt" height="110px">
			<g:Label ui:field="arrow" styleName="{style.arrowImage}" />
		</g:layer>
		<g:layer right="6px" width="33%" top="32pt" height="110px">
			<g:Image styleName="{style.teamImage}" ui:field="bottomTeamImage" />
		</g:layer>
		<g:layer left="0%" width="50%" bottom="6px" height="14pt">
			<g:Label styleName="{style.teamChange}" ui:field="topTeamChange" />
		</g:layer>
		<g:layer left="50%" width="50%" bottom="6px" height="14pt">
			<g:Label styleName="{style.teamChange}" ui:field="bottomTeamChange" />
		</g:layer>
	</g:LayoutPanel>

</ui:UiBinder> 